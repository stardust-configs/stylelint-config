{
  "name": "@stardust-configs/stylelint-config",
  "version": "0.4.0",
  "description": "Shareable Stylelint config",
  "keywords": [
    "stylelint",
    "stylelint-config"
  ],
  "homepage": "https://github.com/stardust-configs/stylelint-config#readme",
  "bugs": {
    "url": "https://github.com/stardust-configs/stylelint-config/issues"
  },
  "license": "MIT",
  "author": "P-Chan",
  "files": [
    "index.js",
    "stylelint.config.js",
    "rules/*"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/stardust-configs/stylelint-config.git"
  },
  "scripts": {
    "format": "prettier --write .",
    "version:major": "standard-version -r major",
    "version:minor": "standard-version -r minor",
    "version:patch": "standard-version -r patch"
  },
  "dependencies": {
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-recess-order": "3.0.0"
  },
  "devDependencies": {
    "@stardust-configs/prettier-config": "0.1.1",
    "prettier": "2.7.1",
    "standard-version": "9.5.0",
    "stylelint": "14.13.0"
  },
  "peerDependencies": {
    "prettier": "^2.0.0",
    "stylelint": "^14.11.0"
  }
}
